{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    /*路径配置*/
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    /*允许js*/
    "noImplicitAny": false,
    "allowJs": true
  },
  "include": [
    "vite.config.ts"
  ]
}
